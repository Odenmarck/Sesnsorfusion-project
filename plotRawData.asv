
x_color = [1 0 0];
y_color = [0 1 0];
z_color = [0 0 1];

% Get values for easier ploting
t = xhat.t;
gyro = meas.gyr;
acc = meas.acc;
mag = meas.mag;

clf
subplot(2,1,1)

hold on
plot_x = plot(t,gyro(1,:),"Color",x_color)
plot_y = plot(t,gyro(2,:),"Color",y_color)
plot_z = plot(t,gyro(3,:),"Color",z_color)

legend(plot_x, 'x', plot_y, "y", plot_z, "z")
ylim('padded')
title("Gyro")